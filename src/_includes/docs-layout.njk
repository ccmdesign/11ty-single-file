---
layout: layout.njk
---

<div class="styleguide-container">
  <aside class="styleguide-sidebar">
    <nav class="styleguide-nav">
      <h3>Components</h3>
      
      <div class="styleguide-nav-section">
        <h4><a href="/docs/subatomic/">Subatomic</a></h4>
        <ul>
          {% for component in collections.docs_subatomic %}
            {% if not component.fileSlug.includes('index') %}
              <li><a href="{{ component.url }}">{{ component.data.title }}</a></li>
            {% endif %}
          {% endfor %}
        </ul>
      </div>
      
      <div class="styleguide-nav-section">
        <h4><a href="/docs/atoms/">Atoms</a></h4>
        <ul>
          {% for component in collections.docs_atoms %}
            {% if not component.fileSlug.includes('index') %}
              <li><a href="{{ component.url }}">{{ component.data.title }}</a></li>
            {% endif %}
          {% endfor %}
        </ul>
      </div>
      
      <div class="styleguide-nav-section">
        <h4><a href="/docs/molecules/">Molecules</a></h4>
        <ul>
          {% for component in collections.docs_molecules %}
            {% if not component.fileSlug.includes('index') %}
              <li><a href="{{ component.url }}">{{ component.data.title }}</a></li>
            {% endif %}
          {% endfor %}
        </ul>
      </div>
      
      <div class="styleguide-nav-section">
        <h4><a href="/docs/organisms/">Organisms</a></h4>
        <ul>
          {% for component in collections.docs_organisms %}
            {% if not component.fileSlug.includes('index') %}
              <li><a href="{{ component.url }}">{{ component.data.title }}</a></li>
            {% endif %}
          {% endfor %}
        </ul>
      </div>
      
      <div class="styleguide-nav-section">
        <h4><a href="/docs/templates/">Templates</a></h4>
        <ul>
          {% for component in collections.docs_templates %}
            {% if not component.fileSlug.includes('index') %}
              <li><a href="{{ component.url }}">{{ component.data.title }}</a></li>
            {% endif %}
          {% endfor %}
        </ul>
      </div>
    </nav>
  </aside>
  
  <main class="styleguide-content">
    {{ content | safe }}
  </main>
</div> 